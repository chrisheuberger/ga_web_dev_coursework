@developers

<table style="width:300px">
  <tr>
    <td><%= @initial %></td>
    <td><% @name %></td>
    <td><% @tagline %></td>
  </tr>
  <tr>
    <td>Eve</td>
    <td>Jackson</td>
    <td>94</td>
  </tr>
</table>

<table>
<% @developers.each do |developer| %>
  <tr>
    <td><%= developer.name.join(' ') %></td>
    <td><%= developer.initials %></td>
    <td><%= [':)', ':(', ';P', ';)', ':D', ':\'('].sample %></td>
    <td><%= developer.tagline %></td>
  <tr>
<% end %>
</table>
